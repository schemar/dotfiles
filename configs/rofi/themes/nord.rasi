* {
  background-color: #4c566a22;
  text-color: #eceff4;
  spacing: 0;
  width: 1000px;
}

#window {
  border: 0;
}

inputbar {
  children: [prompt,entry];
  border: 0 0 1px 0;
  border-color: #d8dee9;
  padding: 16px;
}

prompt {
  padding: -6px 0 0 0;
  font: "MonoLisa Nerd Font 25";
  background-color: #00000000;
}

entry {
  background-color: #00000000;
}

listview {
  columns: 1;
  cycle: true;
  scrollbar: false;
  padding: 16px;
}

element {
  background-color: #00000000;
  border: 0;
  padding: 12px;
}

element selected {
  background-color: #88C0D0;
  text-color: #4c566a;
  border-radius: 8px;
}

element-icon {
  size: 24px;
}

element-text {
   highlight: none;
}
